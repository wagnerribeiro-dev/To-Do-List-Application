{"ast":null,"code":"import axios from 'axios';\nimport AddTask from './AddTask.vue';\nimport TaskItem from './TaskItem.vue';\nimport EditTask from './EditTask.vue';\nexport default {\n  components: {\n    AddTask,\n    TaskItem,\n    EditTask\n  },\n  data() {\n    return {\n      tasks: [],\n      taskToEdit: null\n    };\n  },\n  methods: {\n    fetchTasks() {\n      axios.get('http://localhost:3333/tasks').then(response => {\n        this.tasks = response.data;\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    addTask(task) {\n      axios.post('http://localhost:3333/tasks', task).then(response => {\n        this.tasks.push(response.data);\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    removeTask(id) {\n      axios.delete(`http://localhost:3333/tasks/${id}`).then(response => {\n        this.tasks = this.tasks.filter(task => task.id !== id);\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    editTask(task) {\n      this.taskToEdit = task;\n    },\n    updateTask(task) {\n      axios.put(`http://localhost:3333/tasks/${task.id}`, task).then(response => {\n        const index = this.tasks.findIndex(t => t.id === task.id);\n        this.$set(this.tasks, index, response.data);\n        this.taskToEdit = null;\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    cancelEdit() {\n      this.taskToEdit = null;\n    }\n  },\n  mounted() {\n    this.fetchTasks();\n  }\n};","map":{"version":3,"names":["axios","AddTask","TaskItem","EditTask","components","data","tasks","taskToEdit","methods","fetchTasks","get","then","response","catch","error","console","addTask","task","post","push","removeTask","id","delete","filter","editTask","updateTask","put","index","findIndex","t","$set","cancelEdit","mounted"],"sources":["C:\\Users\\wgrle\\OneDrive\\Documentos\\Projetos\\To-Do-List-Application\\Frontend\\list_application\\src\\components\\TaskList.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h1>Lista de Tarefas</h1>\r\n    <AddTask @add=\"addTask\" />\r\n    <ul>\r\n      <TaskItem \r\n        v-for=\"task in tasks\" \r\n        :key=\"task.id\" \r\n        :task=\"task\" \r\n        @remove=\"removeTask\" \r\n        @edit=\"editTask\"\r\n      />\r\n    </ul>\r\n    <EditTask \r\n      v-if=\"taskToEdit\" \r\n      :task=\"taskToEdit\" \r\n      @save=\"updateTask\" \r\n      @cancel=\"cancelEdit\"\r\n    />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport AddTask from './AddTask.vue';\r\nimport TaskItem from './TaskItem.vue';\r\nimport EditTask from './EditTask.vue';\r\n\r\nexport default {\r\n  components: {\r\n    AddTask,\r\n    TaskItem,\r\n    EditTask\r\n  },\r\n  data() {\r\n    return {\r\n      tasks: [],\r\n      taskToEdit: null\r\n    };\r\n  },\r\n  methods: {\r\n    fetchTasks() {\r\n      axios.get('http://localhost:3333/tasks')\r\n        .then(response => {\r\n          this.tasks = response.data;\r\n        })\r\n        .catch(error => {\r\n          console.error(error);\r\n        });\r\n    },\r\n    addTask(task) {\r\n      axios.post('http://localhost:3333/tasks', task)\r\n        .then(response => {\r\n          this.tasks.push(response.data);\r\n        })\r\n        .catch(error => {\r\n          console.error(error);\r\n        });\r\n    },\r\n    removeTask(id) {\r\n      axios.delete(`http://localhost:3333/tasks/${id}`)\r\n        .then(response => {\r\n          this.tasks = this.tasks.filter(task => task.id !== id);\r\n        })\r\n        .catch(error => {\r\n          console.error(error);\r\n        });\r\n    },\r\n    editTask(task) {\r\n      this.taskToEdit = task;\r\n    },\r\n    updateTask(task) {\r\n      axios.put(`http://localhost:3333/tasks/${task.id}`, task)\r\n        .then(response => {\r\n          const index = this.tasks.findIndex(t => t.id === task.id);\r\n          this.$set(this.tasks, index, response.data);\r\n          this.taskToEdit = null;\r\n        })\r\n        .catch(error => {\r\n          console.error(error);\r\n        });\r\n    },\r\n    cancelEdit() {\r\n      this.taskToEdit = null;\r\n    }\r\n  },\r\n  mounted() {\r\n    this.fetchTasks();\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\nul {\r\n  list-style-type: none;\r\n  padding: 0;\r\n}\r\nli {\r\n  padding: 10px;\r\n  border-bottom: 1px solid #ccc;\r\n}\r\n</style>\r\n"],"mappings":"AAuBA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,OAAM,MAAO,eAAe;AACnC,OAAOC,QAAO,MAAO,gBAAgB;AACrC,OAAOC,QAAO,MAAO,gBAAgB;AAErC,eAAe;EACbC,UAAU,EAAE;IACVH,OAAO;IACPC,QAAQ;IACRC;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACXT,KAAK,CAACU,GAAG,CAAC,6BAA6B,EACpCC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACN,KAAI,GAAIM,QAAQ,CAACP,IAAI;MAC5B,CAAC,EACAQ,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;MACtB,CAAC,CAAC;IACN,CAAC;IACDE,OAAOA,CAACC,IAAI,EAAE;MACZjB,KAAK,CAACkB,IAAI,CAAC,6BAA6B,EAAED,IAAI,EAC3CN,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACN,KAAK,CAACa,IAAI,CAACP,QAAQ,CAACP,IAAI,CAAC;MAChC,CAAC,EACAQ,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;MACtB,CAAC,CAAC;IACN,CAAC;IACDM,UAAUA,CAACC,EAAE,EAAE;MACbrB,KAAK,CAACsB,MAAM,CAAC,+BAA+BD,EAAE,EAAE,EAC7CV,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACN,KAAI,GAAI,IAAI,CAACA,KAAK,CAACiB,MAAM,CAACN,IAAG,IAAKA,IAAI,CAACI,EAAC,KAAMA,EAAE,CAAC;MACxD,CAAC,EACAR,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;MACtB,CAAC,CAAC;IACN,CAAC;IACDU,QAAQA,CAACP,IAAI,EAAE;MACb,IAAI,CAACV,UAAS,GAAIU,IAAI;IACxB,CAAC;IACDQ,UAAUA,CAACR,IAAI,EAAE;MACfjB,KAAK,CAAC0B,GAAG,CAAC,+BAA+BT,IAAI,CAACI,EAAE,EAAE,EAAEJ,IAAI,EACrDN,IAAI,CAACC,QAAO,IAAK;QAChB,MAAMe,KAAI,GAAI,IAAI,CAACrB,KAAK,CAACsB,SAAS,CAACC,CAAA,IAAKA,CAAC,CAACR,EAAC,KAAMJ,IAAI,CAACI,EAAE,CAAC;QACzD,IAAI,CAACS,IAAI,CAAC,IAAI,CAACxB,KAAK,EAAEqB,KAAK,EAAEf,QAAQ,CAACP,IAAI,CAAC;QAC3C,IAAI,CAACE,UAAS,GAAI,IAAI;MACxB,CAAC,EACAM,KAAK,CAACC,KAAI,IAAK;QACdC